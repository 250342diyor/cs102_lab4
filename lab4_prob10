#include <iostream>
using namespace std;

int main(){
    int x, y;
    
    cout << "Enter a point (x y): " << endl;
    
    cin >> x >> y;
    
    if (x * x + y * y <= 100) {
        
        cout << "Point (" << x << "," << y << ") is in circle" << endl;
        
    } else {
        
        cout << "Point (" << x << "," << y << ") is not in circle" << endl;
    }
    
    return 0;
}
