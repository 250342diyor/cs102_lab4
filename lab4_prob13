#include <iostream>
using namespace std;
int main() {
  string l;
  cout << "Enter a color of a traffic light" << endl;
  cin >> l;
  if (l=="g" || l=="G") {
    cout << "Go!" << endl;
  }else if (l=="y" || l=="Y") {
    cout << "Get Ready!" << endl;
  }else if (l=="r" || l=="R") {
    cout << "Stop." << endl;
  }
}


