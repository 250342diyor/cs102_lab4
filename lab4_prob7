#include <iostream>
using namespace std

int main()
{
    int a,b,c;
    cout <<"Enter 3 integer angles: "<< endl;
    cin >> a>>b>>c;
    if (a+b+c==180 && a>0 && b>0 && c>0){
        cout<< "The triangle is valid. "<< endl;
        
    }else{
        cout <<"the triangle is not valid. "<<endl;
    }
    
    return 0;
}
