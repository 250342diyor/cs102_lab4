#include <iostream>
#include <cmath>
using namespace std;
int main() {
    float a,b,c,x1,x2,D,u1,u2,l;
    cout<<"Enter three numbers: ";
    cin>>a>>b>>c;
    D=b*b - 4*a*c;
    u1=-b + sqrt(D);
    u2=-b - sqrt(D);
    l = 2*a;
    x1 = u1/l;
    x2 = u2/l;
    if (x1!=float && x2!=float) {
        cout<<"x1= "<<x1<<" and x2= "<<x2<<endl;
    }else {
        cout<<"Answer does not exist";
    }
}
