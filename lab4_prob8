#include <iostream>
using namespace std;

int main()
{
    char ch;
    
    cout << "Enter a single character: " << endl;
    
    cin >> ch;
    
    if (ch >= 'a' && ch <= 'z') {
        
        cout << "Lowercase alphabet" << endl;
        
    } else if (ch >= 'A' && ch <= 'Z') {
        
        cout << "Uppercase alphabet" << endl;
        
    } else {
        
        cout << "It is not an alphabet" << endl;
    }
    
    return 0;
}
